* Remove rooms

sets va02 erid va00
loop
  setv va03 sins va02 1 " "
  doif va03 ne -1
    setv va04 va03
    subv va03 1
    addv va04 1
    * Get a room ID
    sets va05 subs va02 1 va03
    * Remove this from the list
    setv va06 strl va02
    subv va06 va03
    subv va06 1
    sets va02 subs va02 va04 va06
    doif va05 ne ""
      delr stoi va05
    endi
  endi
untl va03 eq -1
delr stoi va02
delm va00

