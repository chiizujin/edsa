* CUBE

* ACTIVATE 1
scrp 2 21 32206 1
  * Check if param 1 is a creature (call in activate 2)
  doif type _P1_ eq 7
    seta va00 _P1_
  else
    seta va00 from
  endi
  inst
  snde "bbg3"
  setv va01 rand 0 3
  pose va01
  doif va01 eq 3
    lock
    stim writ va00 97 2
    bhvr 0
    inst
    doif type carr eq 7
      targ carr
      drop
      targ ownr
    endi
    slow
    wait 100
    setv va01 0
    loop
      inst
      alph va01 1
      addv va01 1
      slow
    untl va01 gt 256
    kill targ
  else
    stim writ va00 97 1
  endi
endm

